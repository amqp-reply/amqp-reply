{
  "name": "amqp-reply/amqp-reply",
  "type": "library",
  "description": "A variation of 'amqp-messenger' that allows to send a message and wait for a reply",
  "keywords": ["amqp", "rabbitMQ", "reply", "rpc", "messenger", "microservices", "response"],
  "homepage": "https://juanjoconejero.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Juanjo Conejero",
      "email": "juanjoconejero@gmail.com",
      "homepage": "https://juanjoconejero.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-amqp": "*",
    "symfony/amqp-messenger": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/error-handler": "^v6.4",
    "symfony/var-dumper": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "AmqpReply\\AmqpReply\\": "./"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AmqpReply\\AmqpReply\\Tests\\": "Tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1.0"
    }
  }
}
